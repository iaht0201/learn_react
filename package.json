{
  "name": "basic-reactjs-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Khanh.NV",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "rimraf dist && NODE_OPTIONS=--max-old-space-size=8192 vite build --base=./ --mode development",
    "build:packages": "pnpm --filter \"starfish-*\" build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "dev:host": "vite --host",
    "vite:start": "vite",
    "vite:build": "vite build --base=./",
    "build:staging": "rimraf dist && NODE_OPTIONS=--max-old-space-size=8192 vite build --base=./ --mode staging",
    "build:production": "rimraf dist && NODE_OPTIONS=--max-old-space-size=8192 vite build --base=./ --mode production"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "formik": "^2.4.6",
    "i18next": "^23.11.5",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-material-ui-carousel": "^3.4.2",
    "react-paginate": "^8.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "react-slick": "^0.30.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "yarn": "^1.22.22",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "8.57",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.3.2",
    "vite": "^5.3.1"
  }
}
